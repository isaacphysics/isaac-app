{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5"
  },
  "exclude": [
    "node_modules",
  ],
  "packages": {
    "app": {
      "defaultExtension": "ts"
    }
  }
}
